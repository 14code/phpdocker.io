    {{ webserverHostname }}:
      image: phpdockerio/nginx:latest
      container_name: {{ webserverHostname }}
      volumes:
          - ..:{{ workdir }}
          - ./nginx/nginx.conf:/etc/nginx/conf.d/default.conf
      ports:
       - "{{ webserverPort }}:80"
      links:
       - {{ phpFpmHostname }}
